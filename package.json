{
  "name": "podpulse",
  "productName": "PodPulse",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development concurrently \"nodemon ./server/router.js\" \"webpack serve --open\"",
    "electron": "node ./electron.js",
    "server": "nodemon ./server/router.js",
    "dev": "webpack serve",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "test": "jest",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.yourapp.podpulse",
    "files": [
      "build/**/*",
      "server/**/*",
      "main.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.0.1",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@kubernetes/client-node": "^0.22.0",
    "@mui/base": "^5.0.0-beta.59",
    "@mui/icons-material": "^6.1.4",
    "@mui/material": "^6.1.4",
    "@mui/system": "^6.1.4",
    "chart.js": "^4.4.4",
    "cors": "^2.8.5",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "dotenv-webpack": "^8.1.0",
    "express": "^4.21.1",
    "express-prom-bundle": "^7.0.0",
    "html-webpack-plugin": "^5.6.3",
    "node-fetch": "^2.7.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "wait-on": "^8.0.1"
  }
}
